{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { 
      "src": "/api/teachers/",
      "dest": "index.js"
    },
    {
      "src": "/api/students/", 
      "dest": "index.js"
    },
    {
      "src": "/.*",
      "dest": "index.js"
    }
  ]
}
